#include<stdio.h>
void print fibonacci(int n){
	static int n1=0,n2=0,n3;
	if(n>0){
		n3=n1+n2;
		n1=n2;
		n2=n3;
		printf("%d",n3);
		print fibonacci(n-1);
	}
}
int main(){
	int n;
	printf("enter the no of elements: ");
	scanf("%d",&n);
	printf("fibonacci series: ");
	printf("%d,%d",0,1);
	printf fibonacci (n-2);
	return 0;
}
